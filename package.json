{
  "name": "gatsby-starter-skeleton",
  "license": "MIT",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop --host 0.0.0.0",
    "fix": "yarn fix:js && yarn fix:scss && yarn fix:prettier",
    "fix:js": "yarn lint:js --fix",
    "fix:scss": "yarn lint:scss --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:js && yarn lint:scss && yarn lint:prettier",
    "lint:js": "eslint --ignore-path .gitignore . --ext js --ext ts --ext tsx",
    "lint:scss": "stylelint --ignore-path .gitignore 'src/**/*.scss'",
    "lint:prettier": "prettier --ignore-path .gitignore --check .",
    "serve": "gatsby serve --host 0.0.0.0",
    "type-check": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@xstyled/styled-components": "^1.19.1",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "2.29.1",
    "gatsby-image": "2.8.0",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-netlify-cms": "^4.7.0",
    "gatsby-plugin-react-helmet": "3.7.0",
    "gatsby-plugin-sass": "2.8.0",
    "gatsby-plugin-sharp": "2.11.1",
    "gatsby-plugin-sitemap": "2.9.0",
    "gatsby-plugin-styled-components": "^3.7.0",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-remark-prismjs": "^3.10.0",
    "gatsby-source-filesystem": "2.8.0",
    "gatsby-transformer-remark": "^2.13.1",
    "gatsby-transformer-sharp": "2.9.0",
    "marked": "^1.2.7",
    "netlify-cms-app": "^2.14.6",
    "normalize.css": "8.0.1",
    "polished": "^4.0.5",
    "prismjs": "^1.22.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@svgr/webpack": "5.5.0",
    "@types/marked": "^1.2.1",
    "@types/node": "14.14.14",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@types/xstyled__styled-components": "^1.19.1",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "babel-loader": "^8.2.2",
    "eslint": "7.15.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "gatsby-plugin-ts-config": "1.1.5",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "sass": "1.30.0",
    "stylelint": "13.8.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.18.0",
    "typescript": "4.1.3"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": "eslint --fix",
    "src/**/*.scss": [
      "prettier --write",
      "stylelint --fix"
    ]
  }
}
